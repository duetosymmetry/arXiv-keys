{
  "manifest_version": 2,

  "name": "arXiv keys",
  "description": "Adds keyboard navigation keys to the arXiv.",
  "version": "0.9.2.2",

  "icons": {  "48": "icon48.png",
             "128": "icon128.png" },

  "content_scripts": [
    {
      "matches": ["http://arxiv.org/*",
                  "https://arxiv.org/*",
                  "http://*.arxiv.org/*",
                  "https://*.arxiv.org/*",
                  "http://xxx.lanl.gov/*",
                  "https://xxx.lanl.gov/*"],
      "exclude_matches": ["*://*/*.pdf"],
      "css":     ["blitzer.css","arXiv-keys.css"],
      "js":      ["jquery-2.0.0.min.js","jquery-ui-git.js",
                  "categories.js","goto-box.js","arXiv-keys.js"]
    }
  ]
}
